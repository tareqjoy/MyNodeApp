[Unit]
Description=MongoDB Replica Set - Instance 2
After=network.target

[Service]
User=mongodb
ExecStart=/usr/bin/mongod --replSet rs0 --port 27019 --bind_ip 0.0.0.0 --dbpath /srv/mongodb/rs0-2 --oplogSize 128 --logpath /var/log/mongodb/rs0-2.log --logappend
Restart=always

[Install]
WantedBy=multi-user.target
